{
  "name": "docvault",
  "version": "1.0.0",
  "description": "Document Manager",
  "main": "server.js",
  "scripts": {
    "start": "nodemon ./bin/www --exec babel-node",
    "e2e-setup": "node_modules/selenium-standalone/bin/selenium-standalone install",
    "e2e-test": "nightwatch --config nightwatch.conf.js",
    "test": "npm run db:migrate:test && npm run test:clientServer",
    "test:server": "NODE_ENV=test nyc mocha --compilers js:babel-core/register ./server/test/**/*.js --timeout 20000",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:undo:all && NODE_ENV=test sequelize db:seed:all",
    "test:client": "NODE_ENV=test nyc mocha client/test/testSetup.js client/test/**/*",
    "test:clientServer": "NODE_ENV=test nyc mocha server/test/**/*.js client/test/testSetup.js client/test/**/* --timeout 20000",
    "postinstall": "node nightwatch.conf.js"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "client/test",
      "server/test"
    ]
  },
  "author": "blessing philips'",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "draft-convert": "^1.4.7",
    "draft-js": "^0.10.1",
    "env2": "^2.2.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.3.1",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.8.2",
    "node-mocks-http": "^1.6.3",
    "node-sass": "^4.5.3",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-draft-wysiwyg": "^1.10.0",
    "react-materialize": "^1.0.1",
    "react-paginate": "^4.4.3",
    "react-redux": "^5.0.5",
    "react-render-html": "^0.2.0",
    "react-router": "^3.0.5",
    "react-spinner-material": "^1.0.13",
    "react-swal": "^2.0.0",
    "react-test-renderer": "^15.6.1",
    "react-tinymce": "^0.5.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reinstall": "^2.0.0",
    "sass-loader": "^6.0.5",
    "sequelize": "^3.30.4",
    "style-loader": "^0.18.2",
    "swagger-jsdoc": "^1.3.0",
    "sweetalert": "^1.1.3",
    "url-loader": "^0.5.9",
    "webidl-conversions": "^4.0.1",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "whatwg-url": "^6.0.1"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.4",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "cheerio": "^1.0.0-rc.1",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.20.2",
    "faker": "^4.1.0",
    "istanbul": "^0.4.5",
    "jsdom": "^8.5.0",
    "mocha": "^3.4.2",
    "moxios": "^0.4.0",
    "nightwatch": "^0.9.16",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "redux-mock-store": "^1.2.3",
    "selenium-download": "^2.0.10",
    "selenium-standalone": "^6.5.0",
    "sinon": "^2.3.6"
  }
}
