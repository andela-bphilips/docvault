{
  "name": "docvault",
  "version": "1.0.0",
  "description": "Document Manager",
  "main": "server.js",
  "scripts": {
    "start": "nodemon ./bin/www --exec babel-node",
    "test": "npm run db:migrate:test && NODE_ENV=test mocha --compilers js:babel-core/register ./server/test/**/*.js --timeout 20000",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:undo:all && NODE_ENV=test sequelize db:seed:all"
  },
