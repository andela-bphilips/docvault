{
  "name": "docvault",
  "version": "1.0.0",
  "description": "Document Manager",
  "main": "server.js",
  "scripts": {
    "start": "nodemon ./bin/www --exec babel-node",
    "test": "npm run db:migrate:test && NODE_ENV=test nyc mocha --compilers js:babel-core/register ./server/test/**/*.js --timeout 20000",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:undo:all && NODE_ENV=test sequelize db:seed:all"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "client/test",
      "server/test"
    ]
  },
  "author": "blessing philips'",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "draft-convert": "^1.4.7",
    "draft-js": "^0.10.1",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.3.1",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-draft-wysiwyg": "^1.10.0",
    "react-materialize": "^1.0.1",
    "react-paginate": "^4.4.3",
    "react-redux": "^5.0.5",
    "react-render-html": "^0.2.0",
    "react-router": "^3.0.5",
    "react-spinner-material": "^1.0.13",
    "react-swal": "^2.0.0",
    "react-tinymce": "^0.5.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reinstall": "^2.0.0",
    "sass-loader": "^6.0.5",
    "sequelize": "^3.30.4",
    "style-loader": "^0.18.2",
    "sweetalert": "^1.1.3",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.4",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.20.2",
    "faker": "^4.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "react-hot-loader": "^3.0.0-beta.7"
  }
}
